{
  "name": "work-log",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run i18n && ng serve --proxy-config src/proxy.conf.json --open",
    "start:prod": "npm run i18n && ng serve --proxy-config src/proxy.conf.json --open --prod",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "i18n": "gulp i18n",
    "ngcc": "ngcc",
    "clean:nodemodules": "rm -rf node_modules",
    "firstinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "postinstall": "node locale.js && npm run firstinstall",
    "build:prod": "npm run i18n && ng build --prod",
    "build:prod:stats": "npm run i18n && ng build --prod --stats-json",
    "bundle-report": "npm run build:prod:stats && webpack-bundle-analyzer dist/seed/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.0-rc.11",
    "@angular/cdk": "^9.0.0-rc.8",
    "@angular/common": "~9.0.0-rc.11",
    "@angular/compiler": "9.0.0-rc.11",
    "@angular/core": "^9.0.0-rc.11",
    "@angular/forms": "^9.0.0-rc.11",
    "@angular/http": "^7.2.16",
    "@angular/material": "^9.0.0-rc.8",
    "@angular/material-moment-adapter": "^9.0.0-rc.8",
    "@angular/platform-browser": "^9.0.0-rc.11",
    "@angular/platform-browser-dynamic": "^9.0.0-rc.11",
    "@angular/pwa": "^0.803.23",
    "@angular/router": "^9.0.0-rc.11",
    "@angular/service-worker": "^9.0.0-rc.11",
    "@ngrx/effects": "^8.6.0",
    "@ngrx/store": "^8.6.0",
    "@ngrx/store-devtools": "^8.6.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "classlist.js": "^1.1.20150312",
    "geojson": "^0.5.0",
    "hammerjs": "^2.0.8",
    "material-design-icons": "^3.0.1",
    "moment": "^2.24.0",
    "ngx-moment": "^3.5.0",
    "ngx-quicklink": "^0.1.0",
    "ngx-webstorage": "^4.0.2",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "typedarray-methods": "^1.0.1",
    "url-polyfill": "^1.1.7",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.900.0-rc.10",
    "@angular/cli": "^9.0.0-rc.10",
    "@angular/compiler-cli": "^9.0.0-rc.11",
    "@angular/language-service": "^9.0.0-rc.11",
    "@types/jasmine": "^3.5.1",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "~13.5.0",
    "codelyzer": "^5.2.1",
    "gulp": "^4.0.2",
    "gulp-merge-json": "^1.3.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.1",
    "protractor": "~5.4.0",
    "ts-node": "~8.6.2",
    "tslint": "~6.0.0",
    "typescript": "~3.7.5",
    "webpack-bundle-analyzer": "^3.6.0"
  }
}